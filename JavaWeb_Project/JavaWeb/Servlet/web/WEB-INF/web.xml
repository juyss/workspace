<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Servlet01</servlet-name>
        <servlet-class>com.juyss.servlet.Servlet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet01</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>Dispatch01</servlet-name>
    <servlet-class>com.juyss.dispatcher.Dispatch01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dispatch01</servlet-name>
        <url-pattern>/dispatch01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Dispatch02</servlet-name>
        <servlet-class>com.juyss.dispatcher.Dispatch02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dispatch02</servlet-name>
        <url-pattern>/dispatch02</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Dispatch03</servlet-name>
        <servlet-class>com.juyss.dispatcher.Dispatch03</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dispatch03</servlet-name>
        <url-pattern>/dispatch03</url-pattern>
    </servlet-mapping>
</web-app>